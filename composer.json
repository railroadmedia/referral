{
  "name": "railroad/referral",
  "type": "library",
  "description": "Musora referral package",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "guzzlehttp/guzzle": "~6.0",
    "railroad/usora": "2.0.*",
    "railroad/ecommerce": "3.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Referral\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Referral\\Tests\\": "tests/"
    }
  }
}
