{
  "name": "railroad/referral",
  "type": "library",
  "description": "Musora referral package",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "railroad/usora": "3.0.*",
    "railroad/ecommerce": "5.0.*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Referral\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Referral\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
